{
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Interval Braining",
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    }
  },
  "description": "Flashcards.",
